<p id="notice"><%= notice %></p>

<h1>Dog Files</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>User</th>
      <th>Sex</th>
      <th>Weight</th>
      <th>Age</th>
      <th>Breed</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dog_files.each do |dog_file| %>
      <tr>
        <td><%= dog_file.name %></td>
        <td><%= dog_file.user_id %></td>
        <td><%= dog_file.sex %></td>
        <td><%= dog_file.weight %></td>
        <td><%= dog_file.age %></td>
        <td><%= dog_file.breed %></td>
        <td><%= link_to 'Show', dog_file %></td>
        <td><%= link_to 'Edit', edit_dog_file_path(dog_file) %></td>
        <td><%= link_to 'Destroy', dog_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dog File', new_dog_file_path %>
